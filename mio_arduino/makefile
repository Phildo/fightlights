ARDUINO_DIR = /usr/share/arduino

USER_LIB_PATH = $(HOME)/sketchbook/libraries

BOARD_TAG = uno
BOARD_SUB = atmega328

include /usr/share/arduino/Arduino.mk

.PHONY:arduino
arduino:
	cd ..; make arduino

.PHONY:mio
mio:
	cd ..; make mio

.PHONY:gpu
gpu:
	cd ..; make gpu

.PHONY:btn
btn:
	cd ..; make btn

.PHONY:pi
pi:
	cd ..; make pi

